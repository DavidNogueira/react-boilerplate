{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "David's React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "test": "jest ./src",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidNogueira/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "David Nogueira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidNogueira/react-boilerplate/issues"
  },
  "homepage": "https://github.com/DavidNogueira/react-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.4.4",
    "webpack-node-externals": "^1.7.2"
  }
}
