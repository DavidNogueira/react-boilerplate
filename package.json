{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "David's React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test" : "jest ./src"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidNogueira/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "David Nogueira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidNogueira/react-boilerplate/issues"
  },
  "homepage": "https://github.com/DavidNogueira/react-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^22.4.4"
  }
}
